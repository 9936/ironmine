<%= page_title %>
<!--页面结构-->
<div class="page-block">
  <table class="page-block-header">
    <tr>
      <td class="page-block-title">
        <h2 class="block-title"><%= t(:label_chm_change_impact_list) %></h2>
      </td>
      <td class="page-block-button">
        <%= link_to t(:new), {:action => "new"},:class=>"btn"%>
      </td>
      <td class="page-block-help">
        <div id="impactsSearchBox"></div>
      </td>
    </tr>
  </table>
  <div class="page-block-body">
    <div id="impacts">
      <%= datatable("impacts",{:action=>"get_data",:format=>"json"},[],{:paginator_box=>"impactsPaginatorBox",:search_box=>"impactsSearchBox"})%>
    </div>
    <div id="impactsPaginatorBox"></div>
  </div>
</div>