<%= page_title %>
<!--页面结构-->
<div class="page-block">
  <table class="page-block-header">
    <tr>
      <td class="page-block-title">
        <h2 class="block-title"><%= t(:label_chm_advisory_board_list) %></h2>
      </td>
      <td class="page-block-button">
        <%= link_to t(:new), {:action => "new"},:class=>"btn"%>
      </td>
      <td class="page-block-help">
        <div id="advisory_boardsSearchBox"></div>
      </td>
    </tr>
  </table>
  <div class="page-block-body">
    <div id="advisory_boards"></div>
    <%= datatable("advisory_boards",{:action=>"get_data",:format=>"json"},[],{:paginator_box=>"advisory_boardsPaginatorBox",:search_box=>"advisory_boardsSearchBox"})%>
    <div id="advisory_boardsPaginatorBox"></div>
  </div>
</div>


