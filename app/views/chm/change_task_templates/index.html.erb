<%= page_title %>
<!--页面结构-->
<div class="page-block">
  <table class="page-block-header">
    <tr>
      <td class="page-block-title">
        <h2 class="block-title"><%= t(:label_chm_change_task_template_list) %></h2>
      </td>
      <td class="page-block-button">
        <%= link_to t(:new), {:action => "new"},:class=>"btn"%>
      </td>
      <td class="page-block-help">
        <div id="task_templatesSearchBox"></div>
      </td>
    </tr>
  </table>
  <div class="page-block-body">
    <div id="task_templates">
      <%= datatable("task_templates",{:action=>"get_data",:format=>"json"},[],{:paginator_box=>"task_templatesPaginatorBox",:search_box=>"task_templatesSearchBox"})%>
    </div>
    <div id="task_templatesPaginatorBox"></div>
  </div>
</div>