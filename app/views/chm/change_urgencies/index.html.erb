<%= page_title %>

<!--页面结构-->
<div class="page-block">
  <table class="page-block-header">
    <tr>
      <td class="page-block-title">
        <h2 class="block-title"><%= t(:label_chm_change_urgency_list) %></h2>
      </td>
      <td class="page-block-button">
        <%= link_to t(:new), {:action => "new"},:class=>"btn"%>
      </td>
      <td class="page-block-help">
        <div id="urgenciesSearchBox"></div>
      </td>
    </tr>
  </table>
  <div class="page-block-body">
    <div id="urgencies"></div>
    <%= datatable("urgencies",{:action=>"get_data",:format=>"json"},[],{:paginator_box=>"urgenciesPaginatorBox",:search_box=>"urgenciesSearchBox"})%>
    <div id="urgenciesPaginatorBox"></div>
  </div>
</div>