<html>
<head>
  <title>Ironmine</title>

  <script type="text/javascript" src="/javascripts/yui3.3/yui/yui-min.js"></script>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
  <link rel="stylesheet" type="text/css" href="/themes/salesforce2/stylesheets/application.css"></link>
<style type="text/css">
  .setupTab .oLeft .mTreeSelection {
	padding: 0;
    padding-left: 10px;
    text-decoration: none;
}

.setupTab .oLeft .mTreeSelection  .setupNavtree h2,
.sysAdminTab .oLeft .mTreeSelection #AutoNumber5 > .setupNavtree h2 {
	border-width: 0;
	border-top: 1px solid #FFF;
	margin: 0;
	font-size: 1.167em;
	color: #333435;
	padding: 0;
	padding-top: 14px;
	padding-left: 10px;
}

.setupTab .oLeft .mTreeSelection  img:first-child + .setupNavtree h2,
.sysAdminTab .oLeft .mTreeSelection img:first-child + .setupNavtree h2 {
	padding-top: 10px;
	border-width: 0;
}
.mTreeSelection .parent {
	font-weight: bold;
	line-height: 11px;
	padding: 0;
	margin-top: 5px;
	vertical-align: middle;
}
.mTreeSelection .childContainer {
	margin-left: 18px;
	padding-bottom: 10px;
    display: none;

}
.mTreeSelection .childContainer .parent .childContainer {
	padding-bottom: 3px;
}

.mTreeSelection .setupHighlightLeaf a,
.mTreeSelection .setupHighlightFolder a {
	margin: 0;
	padding: 0;
	color: #1797C0;
	font-weight: normal;
}
.mTreeSelection a.setupFolder,
.mTreeSelection a.setupHighlightFolder {
	background-color: transparent;
	margin-left: 6px;
	position: relative;
	top: -1px;
}
.mTreeSelection a.setupHighlightFolder {
	color: #1797C0;
}
.mTreeSelection a.setupHighlightFolder:hover {
	text-decoration: underline;
}
.mTreeSelection .setupImage {
	padding: 0;
}
.setupLeaf a {
    text-decoration: none;
    font-weight: normal;
}
.setupNavtree {
	margin: 14px 0;
	margin-left: -10px;
	border-top: 1px solid #D9D9D9;
}
a.NavTreeCol {
    float:left;
    height: 11px;
    width:11px;
    background: url("/images/setup_plus_lev1.gif") no-repeat scroll 0 0 transparent
}
a.NavTreeExp {
    float:left;
    height: 11px;
    width:11px;
    background: url("/images/setup_minus_lev1.gif") no-repeat scroll 0 0 transparent
}
.setupHighlightLeaf {
    background: url("/img/alohaSkin/setup/setup_selected_bottombg.png") repeat-x scroll left bottom #FFFFFF;
    border-color: #E8E8E9;
    border-style: solid;
    border-width: 1px 0 0 1px;
    line-height: 20px;
    padding-bottom: 3px;
    padding-left: 15px;
    padding-top: 2px;
}
</style>
</head>
<body class="setupTab setup sfdcBody yui3-skin-sam yui-skin-sam">
    <div id="contentWrapper">
        <div id="AppBodyHeader" class="bPageHeader">
            <table id="phHeader" class="phHeader" cellpadding="0" cellspacing="0" border="0">
              <tbody>
                <tr>
                  <td class="left">
                    <img width="175" height="65" title="salesforce.com" id="phHeaderLogoImage" src="/themes/salesforce2/images/sales_logo_aloha_win11.png">
                  </td>
                  <td class="right vtop">
                    <div class="multiforce">
                      <div class="messages"></div>
                      <span class="navLinks">
                        <span class="linkElements">
                            <a href="javascript:void(0);">ivanmarkliu</a>
                            <a href="javascript:void(0);">设置</a>
                        </span>
                      </span>
                      <div id="toolbar"></div>
                    </div>
                  </td>
                </tr>
              </tbody>
            </table>
            <%=level_one_menu%>
            <table id="tabsNewBar" class="tabsNewBar" cellpadding="0" cellspacing="0" border="0">
                <tbody>
                    <tr>
                        <td>
                            <div id="tabNavigation" class="tabNavigation">
                                <div class="tabBarLeft"></div>
                                <table id="tabBar" class="tab" cellpadding="0" cellspacing="0">
                                    <%= level_two_menu %>
                                </table>
                                <div class="tabBarRight"></div>
                            </div>
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>
        <div id="bodyDiv_setup" class="bodyDiv">
            <div id=""></div>
            <div id=""></div>
            <div id="motifCurve">
                <div id="mc_l"></div>
                <div id="mc_r"></div>
            </div>
            <div id="bd_b">
                <div id="bd_bl"></div>
                <div id="bd_br"></div>
            </div>
            <table id="bodyTable" class="outer" width="100%" cellpadding="0" cellspacing="0">
                <tr>
                  <td class="oLeft">
                    <%= sidebar_menu %>
                  </td>
                  <td id="bodyCell" class="oRight">
                    <%= yield %>
                  </td>
                </tr>
            </table>
        </div>
    </div>

</body>
</html>