    <div id="data_area">
        <div class="containerheader">
           <div class="floatleft headertitle"><%=t(:label_icm_rule_setting_list)%></div>
        </div>
        <div id="rule_settings_grid" class="datagrid"
            action_type="ajaxDhtmlxGrid"
            grid_header=" , <%=t(:label_icm_rule_setting_company)%>,<%=t(:label_icm_rule_setting_assignment_process_code)%>,
            <%=t(:label_icm_rule_setting_report_date_changable)%>,<%=t(:label_icm_rule_setting_slove_date_changable)%>,
            <%=t(:label_icm_rule_setting_respond_date_changable)%>,<%=t(:label_icm_rule_setting_auto_assignable)%>,<%=t(:label_icm_rule_setting_auto_closure_days)%>"
            grid_width="20,100,100,100,100,80,80,80,80"
            grid_col_types="ra,ro,ro,ro,ro,ro,ro,ro"
            grid_onRowDblClicked="dblClickHandler"
            grid_OnRowSelected="rowSelectedHandler"
            grid_paging_skin="bricks"
            enable_paging="true, 15, null, 'pagingArea', true, 'infoArea'"
            grid_load="'<%= url_for(:action => "get_data", :controller => "icm/rule_settings", :format=>:json) %>',function(){init('#rule_settings_grid');},'json'">
        </div>
        <span id="pagingArea"></span>&nbsp;<span id="infoArea"></span>
        <span id="recfound"></span>
        <script>
            function dblClickHandler(rId, cInd){
                processDblClicked("container1","rule_setting_edit","<%= url_for(:action => "edit", :controller => "icm/rule_settings", :id => ":id") %>", rId);
            }
            function rowSelectedHandler(id, ind){
                dhtmlx_grid_array['rule_setting_grid'].cells(id, 0).setValue("1");
            }
        </script>
        <div class="containerfooter">
        </div>
    </div>