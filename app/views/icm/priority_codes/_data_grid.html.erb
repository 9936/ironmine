    <div id="data_area">
        <div class="containerheader">
           <div class="floatleft headertitle"><%=t(:label_icm_priority_code_list)%></div>
        </div>
        <div id="priority_codes_grid" class="datagrid"
            action_type="ajaxDhtmlxGrid"
            grid_header=" , <%=t(:label_icm_priority_code_company)%>,<%=t(:label_icm_priority_code_priority_code)%>,
            <%=t(:label_icm_priority_code_low_weight_value)%>,<%=t(:label_icm_priority_code_high_weight_value)%>,<%=t(:multilingual)%>"
            grid_width="20,100,100,100,100,80"
            grid_col_types="ra,ro,ro,ro,ro,link"
            grid_onRowDblClicked="dblClickHandler"
            grid_OnRowSelected="rowSelectedHandler"
            grid_paging_skin="bricks"
            enable_paging="true, 15, null, 'pagingArea', true, 'infoArea'"
            grid_load="'<%= url_for(:action => "get_data", :controller => "icm/priority_codes", :format=>:json) %>',function(){init('#priority_codes_grid');},'json'">
        </div>
        <span id="pagingArea"></span>&nbsp;<span id="infoArea"></span>
        <span id="recfound"></span>
        <script>
            function dblClickHandler(rId, cInd){
                processDblClicked("container1","priority_code_edit","<%= url_for(:action => "edit", :controller => "icm/priority_codes", :id => ":id") %>", rId);
            }
            function rowSelectedHandler(id, ind){
                dhtmlx_grid_array['priority_codes_grid'].cells(id, 0).setValue("1");
            }
        </script>
        <div class="containerfooter">
        </div>
    </div>