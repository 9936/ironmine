    <div id="data_area">
        <div class="containerheader">
           <div class="floatleft headertitle"><%=t(:label_icm_impact_range_list)%></div>
        </div>
        <div id="impact_ranges_grid" class="datagrid"
            action_type="ajaxDhtmlxGrid"
            grid_header=" , <%=t(:label_icm_impact_range_name)%>,<%=t(:label_icm_impact_range_impact_code)%>,
            <%=t(:label_icm_impact_range_company)%>,<%=t(:label_icm_impact_range_weight_values)%>,<%=t(:multilingual)%>"
            grid_width="20,100,100,100,100,80"
            grid_col_types="ra,ro,ro,ro,ro,link"
            grid_onRowDblClicked="dblClickHandler"
            grid_OnRowSelected="rowSelectedHandler"
            grid_paging_skin="bricks"
            enable_paging="true, 15, null, 'pagingArea', true, 'infoArea'"
            grid_load="'<%= url_for(:action => "get_data", :controller => "icm/impact_ranges", :format=>:json) %>',function(){init('#impact_ranges_grid');},'json'">
        </div>
        <span id="pagingArea"></span>&nbsp;<span id="infoArea"></span>
        <span id="recfound"></span>
        <script>
            function dblClickHandler(rId, cInd){
                processDblClicked("container1","impact_range_edit","<%= url_for(:action => "edit", :controller => "icm/impact_ranges", :id => ":id") %>", rId);
            }
            function rowSelectedHandler(id, ind){
                dhtmlx_grid_array['impact_ranges_grid'].cells(id, 0).setValue("1");
            }
        </script>
        <div class="containerfooter">
        </div>
    </div>