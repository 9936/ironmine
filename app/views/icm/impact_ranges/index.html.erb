<%= common_title%>

<!--页面结构-->
<div class="page-block">
  <table class="page-block-header">
    <tr>
      <td class="page-block-title">
        <h2 class="block-title"><%= t(:label_icm_impact_range_list) %></h2>
      </td>
      <td class="page-block-button">
        <%= link_to t(:new), {:action => "new"},:class=>"btn"%>
      </td>
      <td class="page-block-helper">
        <div id="impactRangesSearchBox"></div>
      </td>
    </tr>
  </table>
  <div class="page-block-body">
    <div id="pizza">
      <%= datatable("pizza",{:action=>"get_data",:format=>"json"},[],{:paginator_box=>"impactPaginator",:search_box=>"impactRangesSearchBox"})%>
    </div>
    <div id="impactPaginator"></div>
  </div>
</div>
