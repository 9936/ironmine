<%= page_title %>

<!--页面结构-->
<div class="page-block">
  <table class="page-block-header">
    <tr>
      <td class="page-block-title">
        <h2 class="block-title"><%= t(:label_icm_incident_category_list) %></h2>
      </td>
      <td class="page-block-button">
        <%= link_to t(:new), {:action => "new"},:class=>"btn"%>
      </td>
      <td class="page-block-helper">
        <div id="incident_categoriesSearchBox"></div>
      </td>
    </tr>
  </table>
  <div class="page-block-body">
    <div id="incident_categories">
      <%= datatable("incident_categories",{:action=>"get_data",:format=>"json"},[],{:paginator_box=>"incident_categoriesPaginatorBox",:search_box=>"incident_categoriesSearchBox"})%>
    </div>
    <div id="incident_categoriesPaginatorBox"></div>
  </div>
</div>


