<%= common_title%>

<!--页面结构-->
<div class="page-block">
  <table class="page-block-header">
    <tr>
      <td class="page-block-title">
        <h2 class="block-title"><%= t(:label_icm_urgence_code_list) %></h2>
      </td>
      <td class="page-block-button">
        <%= link_to t(:new), {:action => "new"},:class=>"btn"%>
      </td>
      <td class="page-block-helper">
        <div id="urgenceCodesSearchBox"></div>
      </td>
    </tr>
  </table>
  <div class="page-block-body">
    <div id="urgence_code">
      <%= datatable("urgence_code",{:action=>"get_data",:format=>"json"},[], {:paginator_box=>"urgencePaginator",:search_box=>"urgenceCodesSearchBox"})%>
    </div>
    <div id="urgencePaginator"></div>
  </div>
</div>
