<%= common_title %>

<!--页面结构-->
<div class="page-block">
  <table class="page-block-header">
    <tr>
      <td class="page-block-title">
        <h2 class="block-title"><%= t(:label_icm_incident_phase_list) %></h2>
      </td>
      <td class="page-block-button">
        <%= link_to t(:new), {:action => "new"},:class=>"btn"%>
      </td>
      <td class="page-block-help">
        <div id="incidentPhasesSearchBox"></div>
      </td>
    </tr>
  </table>
  <div class="page-block-body">
    <div id="phase">
      <%= datatable("phase",{:action=>"get_data",:format=>"json"},[],:paginator_box=>"phasePaginator",:search_box=>"incidentPhasesSearchBox")%>
    </div>
    <div id="phasePaginator"></div>
  </div>
</div>

