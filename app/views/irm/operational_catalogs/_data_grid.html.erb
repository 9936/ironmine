    <div id="data_area">
        <div class="containerheader">
           <div class="floatleft headertitle"><%=t(:label_irm_operational_catalog_list)%></div>
        </div>
        <div id="operational_catalogs_grid" class="datagrid"
            action_type="ajaxDhtmlxGrid"
            grid_header=" , <%=t(:label_irm_operational_catalog_code)%>,<%=t(:label_irm_operational_catalog_name)%>, <%= t(:label_irm_operational_catalog_type)%>,
            <%=t(:label_irm_operational_catalog_segment1)%>,<%=t(:label_irm_operational_catalog_segment2)%>,
            <%=t(:label_irm_operational_catalog_segment3)%>,<%=t(:label_irm_permission_status_code)%>,<%=t(:multilingual)%>"
            grid_width="20,100,100, 100,100,100,100,80,80"
            grid_col_types="ra,ro,ro,ro,ro,ro,ro,ro,link"
            grid_onRowDblClicked="dblClickHandler"
            grid_OnRowSelected="rowSelectedHandler"
            grid_paging_skin="bricks"
            enable_paging="true, 15, null, 'pagingArea', true, 'infoArea'"
            grid_load="'<%= url_for(:action => "get_data", :controller => "irm/operational_catalogs", :format=>:json) %>',function(){init('#operational_catalogs_grid');},'json'">
        </div>
        <span id="pagingArea"></span>&nbsp;<span id="infoArea"></span>
        <span id="recfound"></span>
        <script>
            function dblClickHandler(rId, cInd){
                processDblClicked("container1","operational_catalog_edit","<%= url_for(:action => "edit", :controller => "irm/operational_catalogs", :id => ":id") %>", rId);
            }
            function rowSelectedHandler(id, ind){
                dhtmlx_grid_array['operational_catalogs_grid'].cells(id, 0).setValue("1");
            }
        </script>
        <div class="containerfooter">
        </div>
    </div>