<div class="container">
    <div class="containerheader">
     <div class="floatleft headertitle"><%=t(:label_irm_lookup_value_lookup_code)%></div>
    </div>
    <div id="lookup_value_grid" style="width:100%; height:200px; background-color:white;"></div>
    <script type="text/javascript" charset="utf-8">
    var lookup_value_grid = new dhtmlXGridObject('lookup_value_grid');
    lookup_value_grid.setHeader("<%=t(:label_lookup_types)%>,<%=t(:label_irm_lookup_value_lookup_code)%>,<%=t(:label_meaning)%>,"+
                                "<%=t(:label_description)%>,<%=t(:label_status_code)%>,<%=t(:multilingual)%>");
    lookup_value_grid.setInitWidths("100,100,100,100,100,*");
    lookup_value_grid.setColTypes("ro,ro,ro,ro,ro,img");//set column types
    init_grid(lookup_value_grid);
    lookup_value_grid.load("/lookup_types/get_lookup_types","json");
    </script>
    <div class="containerfooter">
        <div class="button floatright" id = "id_lookup_code_submit"><a href="#" onclick="saveRow();" ><%= t(:save) %></a>
        </div>
        <div class="button floatright"><a href="javascript:void(0);"><%= t(:cancel) %></a>
        </div>
        <div class="button floatright" id = "id_add"><a href="#" onclick="addRow(lookup_value_grid);"><%= t(:add) %></a>
        </div>
        <div class="button floatright"><a href="javascript:void(0);" onclick="removeRow(lookup_value_grid);"><%= t(:delete) %></a>
        </div>
    </div>
</div>