<div class="bPageTitle">
  <div class="ptBody">
    <div class="content">
      <h1 class="pageType"></h1>
      <h2 class="pageDescription"><%= t(:label_irm_lookup_type_new) %></h2>
      <div class="blank">&nbsp;</div>
    </div>
    <div class="links">
      <a href="javascript:void(0);" class="configLinks"><%=t(:label_help_about_page)%></a>
    </div>
    <div class="ptBreadcrumb"></div>
  </div>
</div>
<div class="listRelatedObject setupBlock">
    <div class="bPageBlock">
      <div class="pbHeader">
        <table cellpadding="0" cellspacing="0" border="0">
          <tbody>
            <tr>
              <td class="pbTitle"><h2 class="mainTitle"><%= t(:label_irm_lookup_type_list) %></h2></td>
              <td id="topButtonRow" class="pbButton">
                <div class="button"><%= link_to t(:new), {:action => "new"}%></div>
              </td>
            </tr>
          </tbody>
        </table>
      </div>
      <div class="pbBody">
        <div id="lookup_type" class="datatable" style="width:100%;">
          <div id="id" style="display:none">
            <%= link_to(t(:edit),{:action=>"edit",:id=>"{id}"}) %>|<%= link_to(t(:show),{:action=>"show",:id=>"{id}"}) %>
          </div>
        </div>
        <%= datatable("lookup_type",url_for(:controller=>"irm/lookup_types",:action=>"get_lookup_types",:format=>"json"),
                  [{:key=>"id",:label=>t(:edit),:formatter=>"Y.irm.template",:width=>"50%"},
                   {:key=>"lookup_level",:label=>t(:label_lookup_level),:sortable=>true},
                   {:key=>"lookup_type",:label=>t(:label_lookup_types_code),:sortable=>true},
                   {:key=>"meaning",:label=>t(:label_meaning),:sortable=>true},
                   {:key=>"description",:label=>t(:label_description),:sortable=>true},
                   {:key=>"status_meaning",:label=>t(:label_status_code),:sortable=>true}],
                   {:row_perpage=>20})%>
      </div>
    </div>
</div>