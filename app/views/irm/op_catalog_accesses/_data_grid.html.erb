    <div id="data_area">
        <div class="containerheader">
           <div class="floatleft headertitle"><%=t(:label_irm_op_catalog_access)%></div>
        </div>
        <div id="op_catalog_accesses_grid" class="datagrid"
            action_type="ajaxDhtmlxGrid"
            grid_header=" , <%=t(:label_irm_company_name)%>,<%=t(:active)%>, <%= t(:label_irm_op_catalog_access_icm)%>,
            <%=t(:label_irm_op_catalog_access_csi)%>,<%=t(:label_irm_op_catalog_access_skm)%>"
            grid_width="20,100,100, 80,80,80"
            grid_col_types="ra,ro,ro,ch,ch,ch"
            grid_onRowDblClicked="dblClickHandler"
            grid_OnRowSelected="rowSelectedHandler"
            grid_paging_skin="bricks"
            enable_paging="true, 15, null, 'pagingArea', true, 'infoArea'"
            grid_load="'<%= url_for(:action => "get_data", :controller => "irm/op_catalog_accesses", :format=>:json) %>',function(){init('#op_catalog_accesses_grid');},'json'">
        </div>
        <span id="infoArea"></span>
        <span id="recfound"></span>
        <script>
            function rowSelectedHandler(id, ind){
                dhtmlx_grid_array['op_catalog_accesses_grid'].cells(id, 0).setValue("1");
            }
        </script>
        <div class="containerfooter">
          <div class="floatleft"><span id="pagingArea"></span></div>
        </div>
    </div>