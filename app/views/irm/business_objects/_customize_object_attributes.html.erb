<!--  irm datatable      -->
 <div class="irm-datatable">
   <div>
       <table>
         <thead>
           <tr>
             <th style="width:70px;">
               <div><%= t(:operation) %></div>
             </th>
             <th>
               <div><%= t(:label_irm_object_attribute_name) %></div>
             </th>
             <th>
               <div><%= t(:label_irm_object_attribute_attribute_name) %></div>
             </th>
             <th>
               <div><%= t(:label_irm_object_attribute_category) %></div>
             </th>
             <th style="width:80px;">
               <div><%= t(:label_irm_object_attribute_data_type) %></div>
             </th>

             <th style="width:50px;">
               <div><%= t(:label_irm_object_attribute_filter_flag) %></div>
             </th>
           </tr>
         </thead>
         <tbody>
         <% datas.each do |data| %>
         <tr id="<%=data[:id]%>">
           <td>
             <div>
               <%= link_to(t(:edit),{:controller => "irm/object_attributes", :action => "edit", :bo_id => @business_object.id,:id=>data[:id]}) %>
               <%= link_to(t(:delete),{:controller => "irm/object_attributes", :action => "destroy", :bo_id => @business_object.id,:id=>data[:id]},{:method => :delete,:confirm => t(:label_are_you_sure)}) %>
             </div>
           </td>
           <td>
             <div>
               <%= link_to(data[:name],{:controller => "irm/object_attributes", :action => "show", :bo_id => @business_object.id,:id=>data[:id]}) %>
             </div>
           </td>
           <td>
             <div>
               <%= data[:attribute_name] %>
             </div>
           </td>
           <td>
             <div>
               <%= show_object_attribute_category(data) %>
             </div>
           </td>
           <td>
             <div>
               <%= data[:data_type] %>
             </div>
           </td>
           <td>
             <div>
               <%= check_img(data[:filter_flag]) %>
             </div>
           </td>
         </tr>
         <% end -%>
         </tbody>
       </table>
   </div>
 </div>
<!--  end irm datatable      -->