<%= form_for(@service_catalog,:url=>{:action=>"create"},:builder => LabellingFormBuilder) do |f| %>
<div id="ep" class="bEditBlock bPageBlock">
  <div class="pbHeader">
    <table cellpadding="0" cellspacing="0" border="0">
      <tbody>
        <tr>
          <td class="pbTitle"><h2 class="mainTitle"><%=t(:label_slm_service_catalog_new)%></h2></td>
          <td id="topButtonRow" class="pbButton">
            <div class="button"><%= f.submit t(:save)%></div>
            <div class="button"><%= link_back t(:cancel)%></div>
          </td>
        </tr>
      </tbody>
    </table>
  </div>
  <div class="pbBody">
    <%= error_for @service_catalog %>
    <div id="head_1_ep" class="pbSubheader first tertiaryPalette">
      <span class="pbSubExtra">
        <span class="requiredLegend">
          <span class="requiredExampleOuter">
            <span class="requiredExample">&nbsp;</span>
          </span><span class="requiredMark">*</span>
          <span class="requiredText"> = <%=t(:label_is_required)%></span>
        </span>
      </span>
      <h3><%= t(:label_common_info) %></h3>
    </div>
    <div class="pbSubsection">
      <table class="detailList" cellpadding="0" cellspacing="0" border="0">
        <tbody>

            <tr>
              <td class="labelCol"><label for="service_catalog_service_catalog_code"><%=t(:label_slm_service_catalog_catalog_code)%></label></td>
              <td class="dataCol"><%=f.text_field :catalog_code, :required=>true ,:irm_uppercase=>true,:id=>"service_catalog_service_catalog_code"%></td>
              <td class="labelCol"><label for="service_catalog_service_category_code">
                <%=t(:label_slm_service_catalog_service_category_code)%></label></td>
              <td class="dataCol"><%=f.select :service_category_code,
                                      options_from_collection_for_select(available_service_categories,:category_code,:wrap_name),{},
                                             :required=>true ,:id=>"service_catalog_service_category_code"%></td>
            </tr>

            <tr>
              <td class="labelCol"><label for="service_catalog_name"><%=t(:label_slm_service_catalog_name)%></label></td>
              <td class="dataCol"><%=f.text_field :name, :required=>true ,:id=>"service_catalog_name"%></td>
              <td class="labelCol"><label for="service_catalog_priority_code">
                <%=t(:label_slm_service_catalog_priority_code)%></label></td>
              <td class="dataCol"><%=f.select :priority_code,
                                      available_lookup_type('SLM_CATALOG_PRIORITY'),{},
                                             :required=>true ,:id=>"service_catalog_priority_code"%></td>
            </tr>

            <tr>
              <td class="labelCol"><label for="service_catalog_active_start_date"><%=t(:label_slm_service_catalog_active_start_date)%></label></td>
              <td class="dataCol"><%= f.text_field :active_start_date, :id=>"service_catalog_active_start_date"%></td>
              <td class="labelCol"><label for="service_catalog_slm_code"><%=t(:label_slm_service_catalog_slm_code)%></label></td>
              <td class="dataCol"><%=f.select :slm_code,
                                      available_lookup_type('SLM_CATALOG_PRIORITY'),{},
                                             :id=>"service_catalog_slm_code"%></td>
            </tr>

            <tr>
              <td class="labelCol"><label for="service_catalog_active_end_date"><%=t(:label_slm_service_catalog_active_end_date)%></label></td>
              <td class="dataCol"><%= f.text_field :active_end_date, :id=>"service_catalog_active_end_date"%></td>
              <td class="labelCol"><label for="service_catalog_statistics_api"><%=t(:label_slm_service_catalog_statistics_api)%></label></td>
              <td class="dataCol"><%=f.text_field :statistics_api ,:id=>"service_catalog_statistics_api"%></td>
            </tr>

            <tr>
              <td class="labelCol"><label><%=(t :label_slm_service_catalog_service_owner)%></label></td>
              <td class="dataCol"><%=f.select :service_owner_id,available_service_owner,{},{:required=>true}%></td>
              <td class="labelCol"><label><%=(t :label_slm_service_catalog_external_system_code)%></label></td>
              <td class="dataCol"><%=f.blank_select :external_system_code,options_from_collection_for_select(available_external_systems,:external_system_code,:wrap_system_name),
                                                    {},{:id=>"external_system_code",:required=>true}%></td>
            </tr>

            <tr>
              <td class="labelCol"><label for="service_catalog_description"><%=t(:label_slm_service_catalog_description)%></label></td>
              <td class="dataCol"><%= f.text_area :description, :rows => 4,:id=>"service_catalog_description"%></td>
              <td class="labelCol empty">&nbsp;</td>
              <td class="dataCol empty">&nbsp;</td>
            </tr>

        </tbody>
      </table>
    </div>
  </div>
  <div class="pbBottomButtons">
    <table cellpadding="0" cellspacing="0" border="0">
      <tbody>
        <tr>
          <td class="pbTitle"></td>
          <td id="bottomButtonRow" class="pbButtonb">
            <div class="button"><%= f.submit t(:save)%></div>
            <div class="button"><%= link_back t(:cancel)%></div>
          </td>
        </tr>
      </tbody>
    </table>
  </div>
</div>
<% end %>
