<%= page_title %>
<!--页面结构-->
<div class="page-block">
  <table class="page-block-header"><tr>
    <td class="page-block-title">
      <h2 class="block-title"><%= t(:label_slm_service_category_list) %></h2>
    </td>
    <td class="page-block-button">
      <%= link_to t(:new), {:action => "new"},:class=>"btn"%>
    </td>
    <td class="page-block-help">
      <div id="service_categorysSearchBox"></div>
    </td>
  </tr></table>
  <div class="page-block-body">
    <div id="service_categorys"></div>
    <%= datatable("service_categorys",{:action=>"get_data",:format=>"json"},[],{:search_box=>"service_categorysSearchBox",:paginator_box=>"service_categorysPaginatorBox"})%>
    <div id="service_categorysPaginatorBox"></div>
  </div>
</div>


