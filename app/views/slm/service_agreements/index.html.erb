<%= page_title %>
<!--页面结构-->
<div class="page-block">
  <table class="page-block-header"><tr>
    <td class="page-block-title">
      <h2 class="block-title"><%= t(:label_slm_service_agreement_list) %></h2>
    </td>
    <td class="page-block-button">
      <%= link_to t(:new), {:action => "new"},:class=>"btn"%>
    </td>
    <td class="page-block-help">
      <div id="service_agreementsSearchBox"></div>
    </td>
  </tr></table>
  <div class="page-block-body">
    <div id="service_agreements"></div>
    <%= datatable("service_agreements",{:action=>"get_data",:format=>"json"},[],{:search_box=>"service_agreementsSearchBox",:paginator_box=>"service_agreementsPaginatorBox"})%>
    <div id="service_agreementsPaginatorBox"></div>
  </div>
</div>


