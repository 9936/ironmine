<div id="reportChart"></div>

<!--  irm datatable      -->
 <div class="irm-datatable">
   <div>
       <table count="<%=datas[:datas].size%>" style="width: auto;">
         <thead>
         <tr>
           <th colspan=<%= datas[:headers].length %><%=t(:label_report_icm_incident_report_date)+':'+Date.today.to_s  %>
             <br/>
             <%= "#{t(:label_statistic_date)}:#{datas[:params][:year]}-#{datas[:params][:month]}"%>
           </th>
         </tr>
           <tr>
             <th style="width: 150px">
                 <div><%= datas[:headers][0] %></div>
             </th>
             <th style="width: 200px">
               <div><%= datas[:headers][1] %></div>
             </th>
             <th style="width: 80px">
               <div><%= datas[:headers][2] %></div>
             </th>
           </tr>
         </thead>
         <tbody>
         <% datas[:datas].each do |data| %>
           <tr>
             <% 0.upto(datas[:headers].length-1) do |index| %>
               <td>
                 <div>
                   <%= data[index] %>
                 </div>
               </td>
             <% end %>
           </tr>
         <% end -%>
         </tbody>
       </table>
   </div>
 </div>
<!--  end irm datatable      -->
