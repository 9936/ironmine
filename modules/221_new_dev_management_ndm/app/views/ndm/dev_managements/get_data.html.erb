<%=datatable_view({:count=>@count,:datas=>@datas}) do |builder|%>
    <% builder.column(:module,{:title=>"Module", :width => "60px"})%>
    <% builder.column(:no,{:title=> "Gap No.", :width => "60px"}) do |data|%>
        <%= link_to(data[:no],{:controller=>"ndm/dev_managements",:action=>"show",:id=>data[:id]}) %>
    <% end %>
    <% builder.column(:name,{:title=> "Name", :width => "120px"}) do |data|%>
        <%= link_to(data[:name],{:controller=>"ndm/dev_managements",:action=>"show",:id=>data[:id]}) %>
    <% end %>
    <% builder.column(:description,{:title=>"Description"})%>
    <% builder.column(:priority_name,{:title=>"Priority", :width => "50px"})%>
    <% builder.column(:current_status,{:title=>"Current Status", :width => "150px"})%>
    <% builder.column(:type_name,{:title=>"Type", :width => "80px", :phase => "123"})%>
    <% builder.column(:method_name, {:title=>"Method", :width => "80px"})%>
    <% builder.column(:dev_difficulty_name,{:title=>"Difficulty", :width => "80px"})%>
    <% builder.column(:id, {:title => "Phase", :width => "240px"}) do |data| %>
        <table class="table-icon" style="vertical-align: top;">
          <tr>
            <td class="vt" style="vertical-align: top;">
               <span>
                 <a class="expand-icon" href="javascript:void(0);" title="expand" name="<%= data[:id]%>">expand</a>
                 <a class="collapse-icon" href="javascript:void(0);" title="collapse" name="<%= data[:id]%>">collapse</a>
               </span>
            </td>
            <td class="expends expand <%= data[:id]%>"><%= ndm_build_stage_collapse(data) %></td>
          </tr>
        </table>
    <% end %>
    <% builder.column(:gd_owner, {:title => "Owner", :width => "60px"}) do |data| %>
        <table class="table-icon" style="vertical-align: top;">
          <tr>
            <td class="expends expand <%= data[:id]%>"><%= ndm_build_owner_collapse(data) %></td>
          </tr>
        </table>
    <% end %>
    <% builder.column(:gd_status, {:title => "Status", :width => "90px"}) do |data| %>
        <table class="table-icon" style="vertical-align: top;">
          <tr>
            <td class="expends expand <%= data[:id]%>"><%= ndm_build_status_collapse(data) %></td>
          </tr>
        </table>
    <% end %>
    <% builder.column(:gd_plan_start, {:title => "Plan Start", :width => "80px"}) do |data| %>
        <table class="table-icon" style="vertical-align: top;">
          <tr>
            <td class="expends expand <%= data[:id]%>"><%= ndm_build_ps_collapse(data) %></td>
          </tr>
        </table>
    <% end %>
    <% builder.column(:gd_plan_end, {:title => "Plan End", :width => "80px"}) do |data| %>
        <table class="table-icon" style="vertical-align: top;">
          <tr>
            <td class="expends expand <%= data[:id]%>"><%= ndm_build_pe_collapse(data) %></td>
          </tr>
        </table>
    <% end %>
    <% builder.column(:gd_real_start, {:title => "Real Start", :width => "80px"}) do |data| %>
        <table class="table-icon" style="vertical-align: top;">
          <tr>
            <td class="expends expand <%= data[:id]%>"><%= ndm_build_rs_collapse(data) %></td>
          </tr>
        </table>
    <% end %>
    <% builder.column(:gd_real_end, {:title => "Real End", :width => "80px"}) do |data| %>
        <table class="table-icon" style="vertical-align: top;">
          <tr>
            <td class="expends expand <%= data[:id]%>"><%= ndm_build_re_collapse(data) %></td>
          </tr>
        </table>
    <% end %>
<!-- gd fd fdr td co te si at go -->
<% end %>
