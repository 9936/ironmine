        </td>
      </tr>
      <tr>
        <td class="label-col"><label>Module</label></td>
        <td class="data-col">
          <%= f.select :module, nullable_options_for_select(ndm_get_module_list, @dev_management.module)%>
        </td>
      </tr>
      <tr>
        <td class="label-col"><label>Name</label></td>
        <td class="data-col">
          <%= f.text_field :name, :style => "width: 300px;"%>
        </td>
      </tr>
      <tr>
