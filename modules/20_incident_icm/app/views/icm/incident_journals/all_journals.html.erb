<% content_for :incident_request_all_journals do %>
    <div class="page-block" id="incidentReply"  style="border-bottom: none;">
      <div id="allJournals">
        <div style="padding: 0 0 10px 0; font-weight: bold;">
          <table cellpadding="0" cellspacing="0" border="0">
            <tbody>
            <tr>
              <td>
                <h2 style="font-size: 18px;margin-left: 5px;" id="journalsAllNum"><%= t(:label_icm_incident_all_journal_list) %>&nbsp;(<%= all_journals_size(@incident_request) %>)</h2>
              </td>
            </tr>
            </tbody>
          </table>
        </div>
        <div class="page-block-body">
          <% prepare_request_files(@incident_request) %>
          <div id="allReplies">
            <%= list_all_journals(@incident_request) %>
          </div>
        </div>
      </div>
    </div>
<% end %>
<%= content_for :incident_request_all_journals %>