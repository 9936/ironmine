<%= common_title %>
<!--页面结构-->
<div class="page-block">
  <table class="page-block-header"><tr>
    <td class="page-block-title">
      <h2 class="block-title"><%= t(:label_icm_assign_rule_list) %></h2>
    </td>
    <td class="page-block-button">
      <%= link_to t(:new), {:action => "new"},:class=>"btn"%>
    </td>
    <td class="page-block-help">
      <div id="searchBox"></div>
    </td>
  </tr></table>
  <div class="page-block-body">
      <%= datatable("assign_rules",{:action=>"get_data"},[],{:search_box=>"searchBox",:drag_sort => {:save_url => url_for(:controller => 'icm/assign_rules', :action => 'switch_sequence')} })%>
  </div>
</div>


