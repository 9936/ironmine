<%=datatable_view({:count=>@count,:datas=>@datas}) do |builder|%>
    <% builder.column(:id,{:title=>t(:operation), :width => "80px"}) do |data|%>
        <%= link_to(t(:show),{:controller => "icm/support_groups", :action => "show", :id => data[:id]},:class => "action-link") %>&nbsp;|
        <%= link_to(t(:edit),{:controller => "icm/support_groups", :action => "edit", :id => data[:id]},:class => "action-link") %>
    <% end %>
    <% builder.column(:group_id,{:title=>t(:label_icm_support_group_name),:searchable => true}) do |data| %>
        <%= support_name(data[:group_id]) %>
    <% end %>
    <% builder.column(:oncall_flag,{:title=>t(:label_icm_support_group_oncall_flag)}) do |data| %>
        <%= check_img(data[:oncall_flag]) %>
    <% end %>
    <% builder.column(:vendor_flag,{:title=>t(:label_icm_support_group_vendor_flag)}) do |data| %>
        <%= check_img(data[:vendor_flag]) %>
    <% end %>
    <% builder.column(:assignment_process_code,{:title=>t(:label_icm_support_group_assignment_process_code)}) do |data| %>
        <%=look_up_meaning("ICM_ASSIGN_PROCESS_TYPE", data[:assignment_process_code]) %>
    <% end %>
    <% builder.column(:status_code,{:title=>t(:label_status_code)}) do |data| %>
        <%=look_up_meaning("SYSTEM_STATUS_CODE", data[:status_code]) %>
    <% end %>
<% end %>