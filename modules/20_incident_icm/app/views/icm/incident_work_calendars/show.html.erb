<%= common_title %>
<!--页面结构-->
<div class="show-block">
  <table class="page-block-header"><tr>
    <td class="page-block-title">
      <h2 class="block-title"><%= t(:label_icm_incident_work_calendar) %></h2>
    </td>
    <td class="page-block-button">
      <%= link_to t(:edit), {:action => "new"},:class=>"btn"%>
      <%= link_back t(:back), {},:class=>"btn" %>
    </td>
  </tr></table>
  <div class="page-block-body">
    <table class="show-table">
      <tbody>
      <tr>
        <td class="label-col"><label><%= t(:label_icm_service_agreement_calendar) %></label></td>
        <td class="data-col"><%= Slm::Calendar.multilingual.find(@incident_work_calendar[:calendar_id])[:name]%></td>
        <td class="label-col"></td>
        <td class="data-col"></td>
      </tr>
      <tr>
        <td class="label-col"><label><%= t(:label_icm_service_agreement_time_zone) %></label></td>
        <td class="data-col"><%= ActiveSupport::TimeZone.new(@incident_work_calendar.time_zone).to_s %></td>
        <td class="label-col"></td>
        <td class="data-col"></td>
      </tr>
      </tbody>
    </table>
  </div>
</div>
