<%=datatable_view({:count=>@count,:datas=>@datas}) do |builder|%>
    <% builder.column(:group_id,{:title=>t(:label_icm_support_group_name),:searchable => true}) do |data| %>
        <%= support_name(data[:group_id]) %>
    <% end %>
    <% builder.column(:assignment_process_code,{:title=>t(:label_icm_support_group_assignment_process_code)}) do |data| %>
        <%=look_up_meaning("ICM_ASSIGN_PROCESS_TYPE", data[:assignment_process_code]) %>
    <% end %>
    <% builder.column(:status_code,{:title=>t(:label_status_code)}) do |data| %>
        <%=look_up_meaning("SYSTEM_STATUS_CODE", data[:status_code]) %>
    <% end %>
<% end %>