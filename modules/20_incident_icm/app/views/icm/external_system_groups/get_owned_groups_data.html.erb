<%=datatable_view({:count=>@count,:datas=>@datas}) do |builder|%>
    <% builder.column(:group_name,{:title=>t(:label_icm_support_group_name),:searchable => true}) %>
    <% builder.column(:assignment_process_code,{:title=>t(:label_icm_support_group_assignment_process_code)}) do |data| %>
        <%=look_up_meaning("ICM_ASSIGN_PROCESS_TYPE", data[:assignment_process_code]) %>
    <% end %>
    <% builder.column(:group_status_code,{:title=>t(:label_status_code)}) do |data| %>
        <%=look_up_meaning("SYSTEM_STATUS_CODE", data[:group_status_code]) %>
    <% end %>
<% end %>