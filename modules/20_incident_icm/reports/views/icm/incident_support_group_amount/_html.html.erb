<!--  irm datatable      -->
 <div class="datatable">
   <div>
       <table count="<%=datas[:datas].size%>">
         <thead>
           <tr>
             <% datas[:headers].each do |header| %>
               <th >
                 <div><%= header %></div>
               </th>
             <% end %>
           </tr>
         </thead>
         <tbody>
         <% datas[:datas].each do |data| %>
           <tr>
             <% data.each do |d| %>
               <td>
                 <div>
                   <%= d %>
                 </div>
               </td>
             <% end %>
           </tr>
         <% end -%>
         </tbody>
       </table>
   </div>
 </div>
<!--  end irm datatable      -->
