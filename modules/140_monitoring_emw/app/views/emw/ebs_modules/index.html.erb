<%= common_title %>
<!--页面结构-->
<div class="page-block">
  <table class="page-block-header"><tr>
    <td class="page-block-title">
      <h2 class="block-title"><%= t(:label_emw_ebs_module_list) %></h2>
    </td>
    <td class="page-block-button">
      <%= link_to({:action => "new"},:class=>"btn btn-success") do %>
       <i class="icon-plus icon-white"></i><%= t(:new) %>
      <% end %>
    </td>
    <td class="page-block-help">
      <div id="searchBox"></div>
    </td>
  </tr></table>
  <div class="page-block-body">
      <%= datatable("ebs_modules",{:action=>"get_data"},[],{:search_box=>"searchBox",:paginator_box=>"paginator"})%>
  </div>
</div>


