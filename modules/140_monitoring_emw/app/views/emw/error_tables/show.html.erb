<%= common_title %>
<!--页面结构-->
<div class="show-block">
  <table class="page-block-header"><tr>
    <td class="page-block-title"></td>
    <td class="page-block-button">
      <%= link_to t(:edit), {:action => "edit", :id => @error_table.id, :table_id => @error_table.interface_table_id },:class=>"btn btn-primary"%>
      <%= link_back(t(:cancel), {}, :class => "btn") %>
    </td>
  </tr></table>
  <div class="page-block-body">
    <table class="show-table">
      <tbody>
      <tr>
        <td class="label-col"><label><%= t(:label_emw_error_table_name) %></label></td>
        <td class="data-col"><%= @error_table[:name] %></td>
        <td class="label-col empty">&nbsp;</td>
        <td class="data-col empty">&nbsp;</td>
      </tr>
      <tr>
        <td class="label-col"><label><%= t(:label_emw_error_table_interface_column) %></label></td>
        <td class="data-col"><%= @error_table[:interface_column] %></td>
        <td class="label-col empty">&nbsp;</td>
        <td class="data-col empty">&nbsp;</td>
      </tr>

      <tr>
        <td class="label-col"><label><%= t(:label_emw_error_table_error_code_column) %></label></td>
        <td class="data-col"><%= @error_table[:error_code_column] %></td>
        <td class="label-col empty">&nbsp;</td>
        <td class="data-col empty">&nbsp;</td>
      </tr>

      <tr>
        <td class="label-col"><label><%= t(:label_emw_error_table_error_message_column) %></label></td>
        <td class="data-col"><%= @error_table[:error_message_column] %></td>
        <td class="label-col empty">&nbsp;</td>
        <td class="data-col empty">&nbsp;</td>
      </tr>

      <tr>
        <td class="label-col"><label><%= t(:description) %></label></td>
        <td class="data-col"><%= @error_table[:description] %></td>
        <td class="label-col empty">&nbsp;</td>
        <td class="data-col empty">&nbsp;</td>
      </tr>
      </tbody>
    </table>
  </div>
</div>
