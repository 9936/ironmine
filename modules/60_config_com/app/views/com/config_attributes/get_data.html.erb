<%=datatable_view({:count=>@count,:datas=>@datas.values}) do |builder|%>
  <% builder.column(:id,{:title=>t(:operation), :width => "80px"}) do |data|%>
    <%= link_to(t(:edit),{:action=>"edit",:id=>data[:id]},:class => "action-link") %>&nbsp;|
    <%= link_to(t(:show),{:action=>"show",:id=>data[:id]},:class => "action-link") %>
  <% end %>
  <% builder.column(:code,{:title=>t(:label_com_config_attribute_code),:searchable=>true, :width => "15%"})%>
  <% builder.column(:name,{:title=>t(:label_com_config_attribute_name),:searchable=>true}) do |data|%>
    <%= get_meaning_by_type_code("CONFIG_ATTRIBUTE_INPUT_TYPE", data[:input_type]) %>
  <% end %>
  <% builder.column(:input_type,{:title=>t(:label_com_config_attribute_input_type),:searchable=>true})%>
  <% builder.column(:input_value,{:title=>t(:label_com_config_attribute_input_value)})%>
  <% builder.column(:description,{:title=>t(:label_com_config_attribute_description)})%>
  <% builder.column(:extend,{:title=>t(:label_config_class_attribute_extend)}) do |data|%>
    <%= check_img(data[:from_parent]) %>
  <% end %>
<% end %>
