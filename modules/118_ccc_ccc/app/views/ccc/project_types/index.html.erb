<%= page_title %>

<!--页面结构-->
<div class="page-block">
  <table class="page-block-header">
    <tr>
      <td class="page-block-title">
        <h2 class="block-title"><%= t(:label_ccc_projectType_list) %></h2>
      </td>
      <td class="page-block-button">
        <%= link_to t(:new), {:action => "new"},:class=>"btn"%>
      </td>
      <td class="page-block-help">
        <div id="projectTypesSearchBox"></div>
      </td>
    </tr>
  </table>
  <div class="page-block-body">
    <%= datatable("projectTypes",{:action=>"get_data"},[],{:paginator_box=>"projectTypesPaginatorBox",:search_box=>"projectTypesSearchBox"})%>
  </div>
</div>


