<%=datatable_view({:count=>@count,:datas=>@datas}) do |builder|%>
    <% builder.column(:last_name,{:title=>t(:label_irm_person_last_name), :searchable => true}) %>
    <% builder.column(:first_name,{:title=>t(:label_irm_person_first_name), :searchable => true}) %>
    <% builder.column(:sex,{:title=>t(:label_sug_contact_sex)}) do |data|%>
        <%= data[:sex_name] %>
    <% end %>
    <% builder.column(:mobile_phone,{:title=>t(:label_sug_contact_mobile_phone)})%>
    <% builder.column(:home_phone,{:title=>t(:label_sug_contact_home_phone)}) %>
    <% builder.column(:email_address,{:title=>t(:label_sug_contact_email_address)}) %>
    <% builder.column(:address_name,{:title=>t(:label_sug_contact_default_address)})%>
<% end %>