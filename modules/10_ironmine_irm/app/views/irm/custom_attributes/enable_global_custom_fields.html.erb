<%= common_title(:model_meaning => t(:label_irm_system_custom_attribute_global),:show_data=>@business_object[:name]) %>

<!--页面结构-->
<div class="show-block">
  <table class="page-block-header"><tr>
    <td class="page-block-title">
      <h2 class="block-title"><%= @business_object[:name] %></h2>
    </td>
    <td class="page-block-button">
      <%= link_to t(:back), {:action => "index"},{:class=>"btn"}%>
    </td>
  </tr></table>
  <div class="page-block-body">
    <table class="show-table">
      <tbody>
      <tr>
        <td class="label-col"><label ><%=t(:label_irm_business_object_name)%></label></td>
        <td class="data-col"><%= @business_object[:name] %></td>
        <td class="label-col"><label ><%=t(:label_irm_business_object_description)%></label></td>
        <td class="data-col"><%= @business_object[:description] %></td>
      </tr>
      <tr>
        <td class="label-col"><label ><%=t(:label_irm_business_object_business_object_code)%></label></td>
        <td class="data-col"><%= @business_object.business_object_code %></td>
        <td class="label-col"><label ><%=t(:label_irm_business_object_workflow_flag)%></label></td>
        <td class="data-col"><%= check_img(@business_object.workflow_flag) %></td>
      </tr>
      <tr>
        <td class="label-col"><label ><%=t(:label_irm_business_object_bo_table_name)%></label></td>
        <td class="data-col"><%= @business_object.bo_table_name %></td>
        <td class="label-col"><label ><%=t(:label_irm_business_object_report_flag)%></label></td>
        <td class="data-col"><%= check_img(@business_object.report_flag) %></td>
      </tr>
      <tr>
        <td class="label-col"><label ><%=t(:label_irm_business_object_bo_model_name)%></label></td>
        <td class="data-col"><%= @business_object.bo_model_name %></td>
        <td class="label-col"><label ><%=t(:label_irm_business_object_standard_flag)%></label></td>
        <td class="data-col"><%= check_img(@business_object.standard_flag) %></td>

      </tr>
      <tr>
        <td class="label-col"><div></div></td>
        <td class="data-col"><div></div></td>
        <td class="label-col"><label ><%=t(:label_irm_business_object_auto_generate_flag)%></label></td>
        <td class="data-col"><%= check_img(@business_object.auto_generate_flag) %></td>
      </tr>
      <tr>
        <td class="label-col"><div></div></td>
        <td class="data-col"><div></div></td>
        <td class="label-col"><label ><%=t(:label_irm_business_object_multilingual_flag)%></label></td>
        <td class="data-col"><%= check_img(@business_object.multilingual_flag) %></td>
      </tr>
      </tbody>
    </table>
  </div>
</div>

<!-- 全局自定义字段 -->
<% if editable_custom_flag?(@business_object.bo_model_name) %>
    <!--页面结构-->
    <div class="page-block">
      <table class="page-block-header"><tr>
        <td class="page-block-title">
          <h2 class="block-title"><%= t(:label_irm_global_custom_attributes) %></h2>
        </td>
      </tr></table>
      <div class="page-block-body">
        <%= render :partial=>"global_custom_attributes",:locals=>{:datas=>global_custom_attributes(@business_object.id, params[:sid])}%>
      </div>
    </div>
<% end %>