<%= common_title %>
<!--页面结构-->
<div class="page-block">
  <table class="page-block-header"><tr>
    <td class="page-block-title">
      <h2 class="block-title"><%= t(:label_ldap_auth_header_list) %></h2>
    </td>
    <td class="page-block-button">
      <%= link_to t(:new), { :action => "new"},:class=>"btn"%>
    </td>
    <td class="page-block-help">
      <div id="ldap_auth_headersSearchBox"></div>
    </td>
  </tr></table>
  <div class="page-block-body">
    <div id="ldap_auth_headers"></div>
    <%= datatable("ldap_auth_headers",{:action=>"get_data",:format=>"json"},[],{:search_box=>"ldap_auth_headersSearchBox",:paginator_box=>"ldap_auth_headersPaginator"})%>
    <div id="ldap_auth_headersPaginator"></div>
  </div>
</div>



