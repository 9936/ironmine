<%= common_title %>
<!--页面结构-->
<div class="show-block">
  <table class="page-block-header"><tr>
    <td class="page-block-title">
      <h2 class="block-title"><%= t(:label_irm_attachment_folder) %></h2>
    </td>
    <td class="page-block-button">
      <%= link_to t(:edit), {:action => "edit", :id => @attachment_folder},:class=>"btn"%>
      <%= link_to t(:back), {:action => "index"},:class=>"btn" %>
    </td>
  </tr></table>
  <div class="page-block-body">
    <table class="show-table">
      <tbody>
                  <tr>
            <td class="label-col"><label ><%=t(:label_irm_attachment_folder_name)%></label></td>
            <td class="data-col"><div><%= @attachment_folder.name %></div></td>
            <td class="label-col empty"><div>&nbsp;</div></td>
            <td class="data-col empty"><div>&nbsp;</div></td>
          </tr>
                  <tr>
            <td class="label-col"><label ><%=t(:label_irm_attachment_folder_parent_id)%></label></td>
            <td class="data-col"><div><%= @attachment_folder.parent_id %></div></td>
            <td class="label-col empty"><div>&nbsp;</div></td>
            <td class="data-col empty"><div>&nbsp;</div></td>
          </tr>
                  <tr>
            <td class="label-col"><label ><%=t(:label_irm_attachment_folder_created_by)%></label></td>
            <td class="data-col"><div><%= @attachment_folder.created_by %></div></td>
            <td class="label-col empty"><div>&nbsp;</div></td>
            <td class="data-col empty"><div>&nbsp;</div></td>
          </tr>
                  <tr>
            <td class="label-col"><label ><%=t(:label_irm_attachment_folder_updated_by)%></label></td>
            <td class="data-col"><div><%= @attachment_folder.updated_by %></div></td>
            <td class="label-col empty"><div>&nbsp;</div></td>
            <td class="data-col empty"><div>&nbsp;</div></td>
          </tr>
                  <tr>
            <td class="label-col"><label ><%=t(:label_irm_attachment_folder_created_at)%></label></td>
            <td class="data-col"><div><%= @attachment_folder.created_at %></div></td>
            <td class="label-col empty"><div>&nbsp;</div></td>
            <td class="data-col empty"><div>&nbsp;</div></td>
          </tr>
                  <tr>
            <td class="label-col"><label ><%=t(:label_irm_attachment_folder_updated_at)%></label></td>
            <td class="data-col"><div><%= @attachment_folder.updated_at %></div></td>
            <td class="label-col empty"><div>&nbsp;</div></td>
            <td class="data-col empty"><div>&nbsp;</div></td>
          </tr>
        
      </tbody>
    </table>
  </div>
</div>
