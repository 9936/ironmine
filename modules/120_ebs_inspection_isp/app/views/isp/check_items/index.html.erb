<%= common_title %>
<!--页面结构-->
<div class="page-block">
  <table class="page-block-header">
    <tr>
      <td class="page-block-title">
        <h2 class="block-title"><%= t(:label_isp_check_item_list) %></h2>
      </td>
      <td class="page-block-button">
        <%= link_to t(:new), {:action => "new"}, :class => "btn" %>
      </td>
      <td class="page-block-help">
        <div id="searchItem"></div>
      </td>
    </tr>
  </table>
  <div class="page-block-body">
    <%= datatable("isp_item", {:controller => "isp/check_items",  :action => "get_data"}, [], {:search_box => "searchItem", :paginator_box => "paginatorItem"}) %>
  </div>
</div>