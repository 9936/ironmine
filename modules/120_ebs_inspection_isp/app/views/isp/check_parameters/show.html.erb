<%= common_title %>
<!--页面结构-->
<div class="show-block">
  <table class="page-block-header">
    <tr>
      <td class="page-block-title">
        <h2 class="block-title"><%= t(:label_isp_check_parameter) %></h2>
      </td>
      <td class="page-block-button">
        <%= link_to t(:edit), {:controller => "isp/check_parameters",:action => "edit", :id => @isp_check_parameter}, :class => "btn" %>
        <%= link_to t(:back), {:controller => "isp/programs", :id => @isp_check_parameter.program_id, :action => "show"}, :class => "btn" %>
      </td>
    </tr>
  </table>
  <div class="page-block-body">
    <table class="show-table">
      <tbody>
      <tr>
        <td class="label-col"><label><%= t(:name) %></label></td>
        <td class="data-col"><%= @isp_check_parameter[:name] %></td>
        <td class="label-col empty">&nbsp;</td>
        <td class="data-col empty">&nbsp;</td>
      </tr>
      <tr>
        <td class="label-col"><label><%= t(:label_isp_parameter_param_type) %></label></td>
        <td class="data-col"><%= @isp_check_parameter[:param_type] %></td>
        <td class="label-col empty">&nbsp;</td>
        <td class="data-col empty">&nbsp;</td>
      </tr>
      <tr>
        <td class="label-col"><label><%= t(:label_isp_parameter_data_type) %></label></td>
        <td class="data-col"><%= @isp_check_parameter[:data_type] %></td>
        <td class="label-col empty">&nbsp;</td>
        <td class="data-col empty">&nbsp;</td>
      </tr>
      <tr>
        <td class="label-col"><label><%= t(:label_isp_parameter_object_symbol) %></label></td>
        <td class="data-col"><%= @isp_check_parameter[:object_symbol] %></td>
        <td class="label-col empty">&nbsp;</td>
        <td class="data-col empty">&nbsp;</td>
      </tr>

      <tr>
        <td class="label-col"><label><%= t(:label_isp_parameter_value) %></label></td>
        <td class="data-col"><%= @isp_check_parameter[:value] %></td>
        <td class="label-col empty">&nbsp;</td>
        <td class="data-col empty">&nbsp;</td>
      </tr>

      <tr>
        <td class="label-col"><label><%= t(:description) %></label></td>
        <td class="data-col">
          <div><%= @isp_check_parameter[:description] %></div>
        </td>
        <td class="label-col empty">&nbsp;</td>
        <td class="data-col empty">&nbsp;</td>
      </tr>
      </tbody>
    </table>
  </div>
</div>
