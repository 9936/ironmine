<%= common_title %>
<!--页面结构-->
<div class="show-block">
  <table class="page-block-header"><tr>
    <td class="page-block-title">
      <h2 class="block-title"><%= t(:label_yan_management_show) %></h2>

    </td>
    <td class="page-block-button">
      <%= link_to t(:edit), {:action => "edit", :id => @management.id},:class=>"btn"%>
      <%= link_to t(:back), {:action => "index"},:class=>"btn" %>
      <%#= link_to(t(:CommentsEdit), {:controller=>"blg/comment",:action => "edit",:id=>comment.id,:p_id=>@tests.id},:rel =>"modal",:class=>"action-link") %>
    </td>
  </tr></table>
  <div class="page-block-body">
    <table class="show-table">
      <tbody>

      <tr>
        <td class="label-col"><label><%=(t :management_code)%></label></td>
        <td class="data-col"><%=@management.management_code%></td>
        <td class="label-col empty">&nbsp;</td>
        <td class="data-col empty">&nbsp;</td>
      </tr>
      <tr>
        <td class="label-col"><label><%=(t :name)%></label></td>
        <td class="data-col"><%=@management.name%></td>
        <td class="label-col empty">&nbsp;</td>
        <td class="data-col empty">&nbsp;</td>
      </tr>
      <tr>
        <td class="label-col"><label><%=(t :description)%></label></td>
        <td class="data-col"><%=@management.description%></td>
        <td class="label-col empty">&nbsp;</td>
        <td class="data-col empty">&nbsp;</td>
      </tr>

      </tbody>
    </table>
  </div>
</div>


<script type="text/javascript">
    function updateInstances() {
        var current_target = $("#lookup_type").data("datatable").target;
        current_target.loadPage(1);
    }
</script>