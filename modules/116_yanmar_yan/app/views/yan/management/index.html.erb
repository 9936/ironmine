<%= common_title %>
<!--页面结构-->

<div class="page-block">
  <table class="page-block-header"><tr>
    <td class="page-block-title">
      <h2 class="block-title"><%= t(:label_yan_management_list) %></h2>
    </td>
    <td class="page-block-button">
      <%= link_modal t(:new), {:action => "new"},:class=>"btn"%>
    </td>
