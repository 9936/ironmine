<%=datatable_view({:count=>@count,:datas=>@datas}) do |builder|%>
    <% builder.column(:master_number,{:title=>"No.", :width => "55px",:searchable=>true,:sortable=>true}) do |data|%>
        <%= link_to(data[:master_number],{:controller=>"mam/masters",:action=>"show",:id=>data[:id]}) %>
    <% end %>
    <% builder.column(:master_type,{:title=>"Master Type",:searchable=>true,:width => "100px"})%>
    <% builder.column(:system_name,{:title=>"System",:searchable=>true,:width => "240px"})%>
    <% builder.column(:requester_name,{:title=> "Requester", :searchable=>true, :width => "100px"})%>
    <% builder.column(:supporter_name,{:title=> "Supporter", :searchable=>true, :width => "100px"})%>
    <% builder.column(:status,{:title=> "Status", :searchable=>true, :width => "60px"})%>
<% end %>