<%=datatable_view({:count=>@count,:datas=>@datas}) do |builder|%>
    <% unless @show_form %>
        <% builder.column(:id,{:title=>t(:operation), :width => "20px"}) do |data|%>
            <%= bootstrap_icons("icon-trash",t(:remove),{:action=>"delete_item",:id=> data[:id],:master_id=>@master_id, :format => "js"},
                                :confirm => t(:label_are_you_sure) ,:method => :delete, :remote => true) %>

        <% end %>
    <% end %>
    <% builder.column(:item_code,{:title=>"Item Code",:width => "5%"})%>
    <% builder.column(:interface_flag,{:title=>"Use interface to other system",:width => "10%"})%>
    <% builder.column(:item_description,{:title=>"Item Description"})%>
    <% builder.column(:organization,{:title=>"Organization",:width => "8%"})%>
    <% builder.column(:template_name,{:title=>"Template", :width => "10%"})%>
    <% builder.column(:reference_item,{:title=>"Reference Item", :width => "5%"})%>
    <% builder.column(:primary_unit,{:title=>"Primary Unit of Measure", :width => "10%"})%>
    <% builder.column(:department_name,{:title=>"Department", :width => "10%"})%>
    <% builder.column(:business_unit_name,{:title=>"Business Unit", :width => "10%"})%>
    <% builder.column(:inventory_account_name,{:title=>"Inventory Account", :width => "10%"})%>
    <% builder.column(:inventory_sub_account_name,{:title=>"Inventory Sub-Account", :width => "10%"})%>
    <% builder.column(:sng_name,{:title=>"Serial Number Generation", :width => "10%"})%>
<% end %>
