<%=datatable_view({:count=>@count,:datas=>@datas}) do |builder|%>
    <% builder.column(:id,{:title=>t(:operation), :width => "20px"}) do |data|%>
        <%= bootstrap_icons("icon-trash",t(:remove),{:action=>"delete_support_group",:id=> data[:id],:system_id=>@system.id},
                            :confirm => t(:label_are_you_sure) ,:method => :delete) %>
    <% end %>
    <% builder.column(:support_group_name,{:title=> "Support Group", :searchable => true})%>
<% end %>
