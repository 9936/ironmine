<!--页面结构-->
<div class="page-block">
  <table class="page-block-header"><tr>
    <td class="page-block-title">
      <h2 class="block-title"><%= t(:label_skm_entry_template_element_list) %></h2>
    </td>
    <td class="page-block-button">
      <%= link_to t(:new), {:action => "new"},:class=>"btn"%>
    </td>
    <td class="page-block-help">
      <div id="elementsSearchBox"></div>
    </td>
  </tr></table>
  <div class="page-block-body">
    <%= datatable("entry_elements_list",{:controller => "skm/entry_template_elements", :action=>"get_data"},[],{:search_box => "elementsSearchBox",:paginator_box=>"elementsPaginatorBox"}) %>
  </div>
</div>
