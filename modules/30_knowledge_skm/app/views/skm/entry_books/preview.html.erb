<%= common_title(:action_meaning=>t(:label_skm_entry_book_preview),:show_data=>@entry_book[:name])%>
<% require_jscss(:markdown) %>

<!--页面结构-->
<div class="show-block">
  <table class="page-block-header">
    <tr>
      <td class="page-block-title">
        <h2 class="block-title"></h2>
      </td>
      <td class="page-block-button">
        <%#= link_to t(:label_skm_entry_book_export), {:action => "export", :id => @entry_book},:target => "_blank",:class=>"btn"%>
      </td>
    </tr>
  </table>
  <div class="page-block-body">
    <div class="markdown-body" style="width: 551pt;text-align: left;margin: 0 auto;">
      <%= generate_entry_book(@entry_book.id) %>
    </div>
  </div>
</div>