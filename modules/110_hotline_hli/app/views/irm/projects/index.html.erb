<%= common_title%>
<div class="page-block">
  <table class="page-block-header">
    <tr>
      <td class="page-block-title">
        <h2 class="block-title"><%= t(:label_irm_external_system_list) %></h2>
      </td>
      <td class="page-block-button">
        <%= link_to t(:new), {:action => "new"},:class=>"btn"%>
      </td>
      <td class="page-block-help">
        <div id="external_systemsSearchBox"></div>
      </td>
    </tr>
  </table>
  <div class="page-block-body">
    <div id="priority_code"></div>
    <%= datatable("projects",{:action=>"get_data",:format=>"json"},[],
                  {:paginator_box=>"external_systemsPaginatorBox",:search_box=>"external_systemsSearchBox",:page_size => "50"})%>
    <div id="external_systemsPaginatorBox"></div>
  </div>
</div>


