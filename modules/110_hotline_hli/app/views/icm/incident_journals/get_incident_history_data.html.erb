<%=datatable_view({:count=>@count,:datas=>@datas}) do |builder|%>
    <% builder.column(:created_at,{:title=>t(:label_icm_incident_journal_date), :width => "55px"})%>
    <% builder.column(:created_by_full_name,{:title=>t(:label_icm_incident_journal_created_by), :width => "40px"})%>
    <% builder.column(:property_key,{:title=>t(:label_icm_incident_journal_event), :width => "60px"}) do |data|%>
        <%= I18n.t(("label_icm_incident_request_" + data[:property_key].gsub(/\_id$/, "")).to_sym) %>
    <% end %>
    <% builder.column(:new_value_label,{:title=>t(:label_icm_incident_journal_new_value), :width => "90px"}) do |data| %>
        <%= "#{data[:old_value_label]} ==> #{data[:new_value_label]}"%>
    <% end %>
<% end %>