<div class="sub-page-block">
  <div class="sub-page-block-header">
    <h3 class="sub-page-block-title"><%= phase[:name]%></h3>
  </div>
  <div class="sub-page-block-body">
  <table class="form-table">
     <tbody>
         <%= fields_for "dev_phase[]", phase, :builder => CustomFormBuilder do |p|%>
         <%= p.hidden_field :id%>
         <tr>
            <td class="label-col"><label>Sequence</label></td>
            <td class="data-col">
              <%= p.text_field :display_sequence%>
            </td>
         </tr>
         <% end %>
     </tbody>
  </table>
  </div>
</div>