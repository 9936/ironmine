<%= common_title(:show_data=>@consultants[:cName]) %>
<!--页面结构-->
<div class="show-block">
  <!-- 人员信息 -->
  <table class="page-block-header">
    <tr>
      <td class="page-block-title">
        <h2 class="block-title"><%= t(:label_irm_person_show) %></h2>
      </td>
      <td class="page-block-button">

        <%= link_to t(:new), {:action => "new"},:class=>"btn btn-primary"%>

        <%= link_to t(:back), {:action => "index"},:class=>"btn"%>
      </td>
    </tr>
  </table>

  <div class="page-block-body">
    <table class="show-table">
      <tbody>
      <tr>
        <td class="label-col"><label><%=t(:label_cons_no)%></label></td>
        <td class="data-col"><%= @consultants.cNo%></td>
        <td class="label-col"><label><%=(t :label_cons_name)%></label></td>
        <td class="data-col"><%= @consultants.cName%></td>
      </tr>
      <tr>
        <td class="label-col"><label><%=t(:label_cons_type)%></label></td>
        <td class="data-col"><%= @consultants.cType%></td>
        <td class="label-col"><label><%=t(:label_cons_sex)%></label></td>
        <td class="data-col"><%= @consultants.cSex%></td>
      </tr>
      <tr>
        <td class="label-col"><label><%=t(:label_cons_tel)%></label></td>
        <td class="data-col"><%= @consultants.cTel%></td>
        <td class="label-col"><label><%=(t :label_cons_tel_no)%></label></td>
        <td class="data-col"><%= @consultants.cTelNo%></td>
      </tr>
      <tr>
        <td class="label-col"><label><%=t(:label_cons_mail)%></label></td>
        <td class="data-col"><%= @consultants.cMail%></td>
        <td class="label-col"><label><%=(t :label_cons_fax)%></label></td>
        <td class="data-col"><%= @consultants.cFax%></td>
      </tr>
      <tr>
        <td class="label-col"><label><%=t(:label_cons_module)%></label></td>
        <td class="data-col"><%= @consultants.cModule%></td>
        <td class="label-col"><label><%=(t :label_cons_flag)%></label></td>
        <td class="data-col"><%= @consultants.cFlag%></td>
      </tr>
      <tr>
        <td class="label-col"><label><%=t(:label_cons_comment)%></label></td>
        <td class="data-col"><%= @consultants.cComment%></td>
      </tr>
      </tbody>
    </table>
  </div>
</div>