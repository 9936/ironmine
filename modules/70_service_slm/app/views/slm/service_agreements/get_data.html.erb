<%=datatable_view({:count=>@count,:datas=>@datas}) do |builder|%>
  <% builder.column(:id,{:title=>t(:operation), :width => "80px"}) do |data|%>
    <%= link_to(t(:edit),{:action=>"edit",:id=>data[:id]},:class => "action-link") %>&nbsp;|
    <%= link_to(t(:show),{:action=>"show",:id=>data[:id]},:class => "action-link") %>
  <% end %>
  <% builder.column(:name,{:title=>t(:label_slm_service_agreement_name),:searchable=>true})%>
  <% builder.column(:duration,{:title=>t(:label_slm_service_agreement_duration)}) do |data|%>
        <%= data.duration_meaning %>
  <% end %>
  <% builder.column(:time_zone,{:title=>t(:label_slm_service_agreement_time_zone)})%>
  <% builder.column(:status_code,{:title=>t(:status)}) do |data|%>
        <%= check_img(data[:status_code]) %>
  <% end %>
<% end %>