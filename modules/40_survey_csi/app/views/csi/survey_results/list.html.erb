<%= common_title(:show_data=>"",:action_meaning=>t(:label_csi_survey_result_list)) %>
<div class="page-block">
  <table class="page-block-header">
    <tr>
      <td class="page-block-title">
        <h2 class="block-title"><%= t(:label_csi_survey_response_list) %></h2>
      </td>
      <td class="page-block-button">
        <%= link_to t(:export),{:action => "export", :id => @survey.id,:format=>:xls},:id=>"exportSurvey",:class=>"btn btn-primary",:target=>"_blank"%>
        <%= link_back t(:back),{},:class => "btn cancel"%>
      </td>
      <td class="page-block-help">
        <div id="searchBox"></div>
      </td>
  </tr></table>
  <div class="page-block-body">
    <div id="responses"></div>
    <%= datatable("responses",{:action=>"get_data",:id=>@survey.id,:format=>"json"},[],{:paginator_box=>"paginator",:search_box=>"searchBox"})%>
    <div id="paginator"></div>
  </div>
</div>

