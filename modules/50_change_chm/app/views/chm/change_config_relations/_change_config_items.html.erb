<%=datatable_view({:count=>datas.length,:datas=>datas}) do |builder|%>
  <% builder.column(:id,{:title=>t(:operation), :width => "40px"}) do |data|%>
    <%= link_to(t(:delete),{:controller => "chm/change_config_relations",:action => "destroy", :id=>data.id,:_dom_id=>"changeConfigItem"},{:remote=>true,:back=>true,:class => "action-link", :method => :delete,:confirm => t(:label_are_you_sure) }) %>
  <% end %>
  <% builder.column(:item_number,{:title=>t(:label_com_config_item_number),:searchable=>true})  %>
  <% builder.column(:config_class_name,{:title=>t(:label_com_config_item_config_class),:searchable=>true})%>
  <% builder.column(:managed_group_name,{:title=>t(:label_com_config_item_managed_group),:searchable=>true})%>
  <% builder.column(:managed_person_name,{:title=>t(:label_com_config_item_managed_person)})%>
  <% builder.column(:last_checked_at,{:title=>t(:label_com_config_item_last_checked_at)})%>
<% end %>