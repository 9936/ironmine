<%=datatable_view({:count=>@count,:datas=>@datas}) do |builder|%>
  <% builder.column(:full_name,{:title=>t(:label_irm_person_full_name), :width => "110px",:searchable=>true})%>
  <% builder.column(:organization_name,{:title=>t(:label_irm_organization), :width => "110px"})%>
  <% builder.column(:email_address,{:title=>t(:label_irm_person_email_address)})%>

  <% builder.row do |data|%>
    data-checkbox-id="<%=data[:person_id]%>"
  <% end %>

<% end %>